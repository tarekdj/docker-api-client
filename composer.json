{
    "name": "tarekdj/docker-api-client",
    "license": "MIT",
    "type": "library",
    "description": "A Docker API PHP client",
    "require": {
        "jane-php/open-api-runtime": "^7.2"
    },
    "require-dev": {
        "jane-php/open-api-2": "^7.2",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Tarekdj\\Docker\\ApiClient\\": "src/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit -c phpunit.xml.dist",
        "generate": "vendor/bin/jane-openapi generate"
    }
}
